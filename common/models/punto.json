  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Nombre": {
      "type": "string",
      "required": true
    },
    "Descripcion": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "juegoDePuntos": {
      "type": "hasMany",
