      "foreignKey": "puntoId",
      "options": {
        "nestRemoting": true
      },
      "through": "AsignacionPuntoJuego"
    },
    "alumnoJuegoDePuntos": {
      "type": "hasMany",
      "model": "AlumnoJuegoDePuntos",
      "foreignKey": "puntoId",
      "options": {
        "nestRemoting": true
      },
      "through": "HistorialPuntosAlumno"
    },
    "equipoJuegoDePuntos": {
      "type": "hasMany",
      "model": "EquipoJuegoDePuntos",
      "foreignKey": "puntoId",
      "through": "HistorialPuntosEquipo"
    }
  },
  "acls": [],
  "methods": {}
}
